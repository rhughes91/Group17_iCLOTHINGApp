@model IEnumerable<Group17_iCLOTHINGApp.Models.OrderStatus>

@{
	ViewBag.Title = "Index";
}

<h2>Current Order Statuses</h2>

Thank you for shopping with us!

<table class="table">
	<tr>
		<th>
			@Html.DisplayNameFor(model => model.status)
		</th>
		<th>
			@Html.DisplayNameFor(model => model.statusDate)
		</th>
		<th>
			@Html.DisplayNameFor(model => model.cartID)
		</th>
		<th>
			@Html.DisplayNameFor(model => model.Administrator.adminName)
		</th>
		<th></th>
	</tr>

	@foreach (var item in Model)
	{
		<tr>
			<td>
				@Html.DisplayFor(modelItem => item.status)
			</td>
			<td>
				@Html.DisplayFor(modelItem => item.statusDate)
			</td>
			<td>
				@Html.DisplayFor(modelItem => item.cartID)
			</td>
			<td>
				@Html.DisplayFor(modelItem => item.Administrator.adminName)
			</td>
		</tr>
	}





</table>

<h2>@Html.ActionLink("Return to Home", "Index", "Home")</h2>